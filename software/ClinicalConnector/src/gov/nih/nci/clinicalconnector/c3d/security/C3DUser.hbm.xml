<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
    
<hibernate-mapping default-lazy="false">
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="gov.nih.nci.security.authorization.domainobjects.User" 
    table="CSM_USER" 
>
    
    <id
        name="userId"
        type="java.lang.Long"
        column="USER_ID"
         unsaved-value="0"
        >
     <generator class="native">
		 <param name="sequence">CSM_USER_USER_ID_SEQ</param>
	 </generator>
    </id>

    <property
        name="loginName"
        type="java.lang.String"
        column="LOGIN_NAME"
        not-null="true"
        length="500"
    >      
    </property>
     <property
        name="preMigrationLoginName"
        type="java.lang.String"
        column="PREMGRT_LOGIN_NAME"
        not-null="false"
        length="100"
    >
    </property>
    <property
        name="firstName"
        type="java.lang.String"
        column="FIRST_NAME"
        not-null="true"
        length="100"
    >
    </property>
    <property
        name="lastName"
        type="java.lang.String"
        column="LAST_NAME"
        not-null="true"
        length="100"
    >
        
    </property>
    <property
        name="organization"
        type="java.lang.String"
        column="ORGANIZATION"
        length="100"
    >
           
    </property>
    <property
        name="department"
        type="java.lang.String"
        column="DEPARTMENT"
        length="100"
    >
        
    </property>
    <property
        name="title"
        type="java.lang.String"
        column="TITLE"
        length="100"
    >
        
    </property>
    <property
        name="phoneNumber"
        type="java.lang.String"
        column="PHONE_NUMBER"
        length="15"
    >
        
    </property>
    <property
        name="password"
        type="java.lang.String"
        column="PASSWORD"
        length="100"
    >
        
    </property>
    <property
        name="emailId"
        type="java.lang.String"
        column="EMAIL_ID"
        length="100"
    >
        
    </property>
    <property
        name="startDate"
        type="java.sql.Date"
        column="START_DATE"
        length="10"
    >
        
    </property>
    <property
        name="endDate"
        type="java.sql.Date"
        column="END_DATE"
        length="10"
    >
        
    </property>
    <property
        name="updateDate"
        type="java.sql.Date"
        column="UPDATE_DATE"
        not-null="true"
        length="10"
    >
    </property>
    
    <property 
    	name="migratedFlag" 
    	type="byte" 
    	column="MIGRATED_FLAG" 
    	not-null="true" 
    	length="1" >
    </property>
    
    <set name="groups" lazy="true" table="CSM_USER_GROUP">
           <key column="user_id"/>
           <many-to-many column="group_id" class="gov.nih.nci.security.authorization.domainobjects.Group"/>
     </set>
     
     <joined-subclass name="gov.nih.nci.cdmsconnector.c3d.security.C3DUser"
		table="C3D_USER">
		<key column="USER_ID" />
		<property name="c3dUserName" type="java.lang.String" column="C3D_USERNAME" />
		<property name="c3dPassword" type="java.lang.String" column="C3D_PASSWORD" />
	</joined-subclass>
</class>
</hibernate-mapping>
